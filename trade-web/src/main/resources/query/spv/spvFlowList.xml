	<query  id="ToSpvCashFlowById">
		<searchSql>
			<baseSql>
				<![CDATA[
			      SELECT 
                     sc.PKID, 
                     sc.CASHFLOW_APPLY_ID, 
                     sc.DIRECTION,  
                     sc.VOUCHER_NO, 
                     sc.RECEIVER,  
                     sc.RECEIVER_ACC, 
                     sc.RECEIVER_BANK, 
                     sc.PAYER, 
                     sc.PAYER_ACC, 
                     sc.PAYER_BANK, 
                     sc.FLOW_CONDITION, 
                     sc.INPUT_TIME,
                     sc.AMOUNT
			      FROM  sctrans.T_TO_SPV_CASHFLOW  sc
                   where sc.CASHFLOW_APPLY_ID=#id# and sc.IS_DELETED=0
				]]>
			</baseSql>
		</searchSql>
