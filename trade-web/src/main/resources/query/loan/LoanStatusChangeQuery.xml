<?xml version="1.0" encoding="UTF-8"?>
<querys>
<dicts>
		<customDict id="qqcdConfirmStatus" beanName="qqcdConfirmStatus"></customDict>
		<customDict id="qqcdYuEplusStatus" beanName="qqcdYuEplusStatus"></customDict>
</dicts>
<formatters>
		<formatter id="dateFormatter" ref="" class="com.aist.common.quickQuery.formatter.DateFormatter">
			<properties>
				<property name="pattern">yyyy-MM-dd HH:mm:ss</property>
			</properties>
		</formatter>
		<formatter id="numberFormater" ref="" class="com.aist.common.quickQuery.formatter.NumberFormatter">
			<properties>
				<property name="groupingUsed">true</property>
				<property name="maximumFractionDigits">3</property>
				<property name="maximumIntegerDigits">40</property>
				<property name="minimumFractionDigits">0</property>
				<property name="minimumIntegerDigits">1</property>
			</properties>
		</formatter>
		<formatter id="integerFormater" ref="" class="com.aist.common.quickQuery.formatter.NumberFormatter">
			<properties>
				<property name="maximumFractionDigits">0</property>
			</properties>
		</formatter>
		<formatter id="emailFormater" ref="" class="com.aist.common.quickQuery.formatter.EmailFormatter"/>
	</formatters>
	<query id="loanStatusChangeQuery">
		<searchCondition>
			<condition field="loan_id" name="loanId" label="LoanId" searchType="EQ" showType="" showCallback="" defaultValue=""/>
		</searchCondition>
		<searchSql>
			<baseSql>
				<![CDATA[
			select PKID,LOAN_ID as loanId,ST_FROM as stFrom,ST_TO as stTo
,CHANGE_DATE as changeDate,IS_CONFIRM as isConfirm
 from sctrans.T_TO_LOAN_STATUS_CHANGE where 1=1 
				]]>
			</baseSql>
		</searchSql>
		<grid>
			<table-row>
				<column type="both" name="stFrom" index="" checkbox="" title="状态(从)" customDict="qqcdYuEplusStatus" sysDict="" format="" width="90" isSort="" align="" halign="" resizable=""/>
				<column type="both" name="stTo" index="" checkbox="" title="状态(到)" customDict="qqcdYuEplusStatus" sysDict="" format="" width="90" isSort="" align="" halign="" resizable=""/>
				<column type="both" name="pkid" index="" checkbox="" title="pkid" customDict="" sysDict="" format="" width="90" isSort="" align="" halign="" resizable=""/>
				<column type="both" name="loanId" index="" checkbox="" title="LoanID" customDict="" sysDict="" format="" width="90" isSort="" align="" halign="" resizable=""/>
				<column type="both" name="changeDate" index="" checkbox="" title="变成时间" customDict="" sysDict="" beanFormatter="dateFormatter" width="90" isSort="" align="" halign="" resizable=""/>
				<column type="both" name="isConfirm" index="" checkbox="" title="产品部是否确认" customDict="qqcdConfirmStatus" sysDict="" format="" width="90" isSort="" align="" halign="" resizable=""/>
			</table-row>
		</grid>
	</query>
</querys>