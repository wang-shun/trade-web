<?xml version="1.0" encoding="UTF-8"?>
<querys>
<dicts>
        <customDict id="getUserNameByUserId" beanName="qqcdUserIdName"></customDict>
        <customDict id="getUserPhoneByUserId" beanName="qqcdUserPhone"></customDict>
        <customDict id="getManagerNameByOrgId" beanName="qqcdYuCuiFhjlName"></customDict>
        <customDict id="getManagerPhoneByOrgId" beanName="qqcdYuCuiFhjlPhone"></customDict>
        <customDict id="getParentOrgNameByOrgId" beanName="qqcdYuCuiOrgName"></customDict>
        <customDict id="qqcdDictServCode" beanName="qqcdDictServCode"></customDict>
        <customDict id="qqcdDictApplyStatus" beanName="qqcdDictApplyStatus"></customDict>
		<customDict id="getIsFeeGetNameByCode" beanName="qqcdDictIsFeeGet"></customDict>
</dicts>
<formatters>
		<formatter id="dateFormatter" ref="" class="com.aist.common.quickQuery.formatter.DateFormatter">
			<properties>
				<property name="pattern">yyyy-MM-dd HH:mm:ss</property>
			</properties>
		</formatter>
		<formatter id="dateFormatter1" ref="" class="com.aist.common.quickQuery.formatter.DateFormatter"/>
		
		<formatter id="numberFormater" ref="" class="com.aist.common.quickQuery.formatter.NumberFormatter">
			<properties>
				<property name="groupingUsed">true</property>
				<property name="maximumFractionDigits">3</property>
				<property name="maximumIntegerDigits">40</property>
				<property name="minimumFractionDigits">0</property>
				<property name="minimumIntegerDigits">1</property>
			</properties>
		</formatter>
		<formatter id="integerFormater" ref="" class="com.aist.common.quickQuery.formatter.NumberFormatter">
			<properties>
				<property name="maximumFractionDigits">0</property>
			</properties>
		</formatter>
		<formatter id="emailFormater" ref="" class="com.aist.common.quickQuery.formatter.EmailFormatter"/>
	</formatters>
	<query id="warnListQuery">
		<searchCondition>
            <condition field="propertyInfo.PROPERTY_ADDR" name="propertyAddr" label="物业地址" searchType="LIKE" showType="" showCallback="" defaultValue=""/>
            <condition field="agent.LOAN_SRV_CODE" name="loanSrvCode" label="税费卡" searchType="EQ" showType="" showCallback="" defaultValue=""/>
            <condition field="orgHierarchy.DISTRICT_ID" name="districtOrgId" label="所属区域" searchType="EQ" showType="" showCallback="" defaultValue=""/>
          </searchCondition>
		<searchSql>
			<baseSql>
				<![CDATA[
					SELECT
					    agent.PKID,
						agent.LOAN_SRV_CODE,
						agent.EXECUTOR_ID,
						agent.CONFIRM_STATUS,
						datediff(dd, agent.CONFIRM_TIME, getdate()) AS EXCEED_DAY,
						agent.LAST_EXCEED_EXPORT_TIME,
					    propertyInfo.PROPERTY_ADDR,
					    userOrg.ORG_id as 'PARENT_ORG_NAME',
					    userOrg.ORG_id as 'MANAGER_NAME',
					    userOrg.ORG_id as 'MANAGER_PHONE',
					    agent.EXECUTOR_ID as 'EXECUTOR_PHONE'
					FROM
						sctrans.T_TO_LOAN_AGENT  agent
					INNER JOIN 
					    sctrans.T_TO_PROPERTY_INFO propertyInfo
					ON
					    agent.CASE_CODE = propertyInfo.CASE_CODE
					INNER JOIN 
					    SCTRANS.V_USER_ORG_JOB userOrg
					ON  
					    AGENT.EXECUTOR_ID = userOrg.user_id
					INNER JOIN
				        sctrans.v_yucui_org_hierarchy orgHierarchy
				    ON
				        orgHierarchy.ORG_ID = userOrg.ORG_id
				    WHERE
				        datediff(dd, agent.CONFIRM_TIME, getdate()) > 0
				]]>
			</baseSql>
			<orderBySql>
			  	ORDER BY agent.CONFIRM_TIME DESC,agent.CASE_CODE DESC
			</orderBySql>
			<groupSql>
			</groupSql>
		</searchSql>
		<grid>
			<table-row>
			    <column type="both" name="PKID" index="" checkbox="" display="主键" customDict="" sysDict="" format="" width="0" isSort="" align="" halign="" order="" resizable="" hide="true"/>
				<column type="both" name="LOAN_SRV_CODE" index="" checkbox="" display="产品名称" title="产品名称" customDict="qqcdDictServCode" sysDict="" format="" width="0" isSort="" align="" halign="" order="" resizable=""/>
				<column type="both" name="EXECUTOR_ID" index="" checkbox="" display="执行人" title="执行人" customDict="getUserNameByUserId" sysDict="" format="" width="0" isSort="" align="" halign="" order="" resizable=""/>
				<column type="both" name="EXECUTOR_PHONE" index="" checkbox="" display="执行人电话" title="执行人电话" customDict="getUserPhoneByUserId" sysDict="" format="" width="0" isSort="" align="" halign="" order="" resizable=""/>
				<column type="both" name="MANAGER_NAME" index="" checkbox="" display="经理姓名" title="经理姓名" customDict="getManagerNameByOrgId" sysDict="" format="" width="0" isSort="" align="" halign="" order="" resizable=""/>
				<column type="both" name="MANAGER_PHONE" index="" checkbox="" display="经理电话" title="经理电话" customDict="getManagerPhoneByOrgId" sysDict="" format="" width="0" isSort="" align="" halign="" order="" resizable=""/>
				<column type="both" name="PARENT_ORG_NAME" index="" checkbox="" display="所属区域" title="所属区域" customDict="getParentOrgNameByOrgId" sysDict="" format="" width="0" isSort="" align="" halign="" order="" resizable=""/>
				<column type="both" name="CONFIRM_STATUS" index="" checkbox="" display="确认状态" title="确认状态" customDict="qqcdDictApplyStatus" sysDict="" format="" width="0" isSort="" align="" halign="" order="" resizable="" />
				<column type="both" name="EXCEED_DAY" index="" checkbox="" display="超期天数" title="超期天数" customDict="" sysDict="" format="" width="90" isSort="true" align="" halign="" order="desc" resizable=""/>
				<column type="both" name="LAST_EXCEED_EXPORT_TIME" index="" checkbox="" display="最后导出时间" title="最后导出时间" customDict="" sysDict="" format="" beanFormatter="dateFormatter" width= "90" align="" halign="" resizable=""/>
				<column type="both" name="PROPERTY_ADDR" index="" checkbox="" display="产证地址" title="产证地址" customDict="" sysDict="" format="" width="180" align="" halign="" order="" resizable=""/>
			</table-row>
		</grid>
	</query>
</querys>