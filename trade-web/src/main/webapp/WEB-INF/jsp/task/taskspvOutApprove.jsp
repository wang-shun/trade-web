    		  				parent.$.fancybox.close();
    		  			});	 
    		                }
    		            } ,
				success : function(data) {
				//	alert("数据已保存。");
						window.wxc.success(data.message);
						if(window.opener)
					    {
							 window.close();
							 window.opener.callback();
