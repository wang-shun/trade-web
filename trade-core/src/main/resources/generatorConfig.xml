<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<!-- classPathEntry:数据库的JDBC驱动的jar包地址 -->
<!-- 	 <classPathEntry location="E:\sqljdbc4.jar" 
		/> -->
	<context id="sqlServerTables" targetRuntime="MyBatis3" defaultModelType="flat">
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
		  
		<jdbcConnection driverClass="com.microsoft.sqlserver.jdbc.SQLServerDriver"
			connectionURL="jdbc:sqlserver://10.4.19.22:1433;DatabaseName=sctrans_dev"
			userId="dev" password="dev.good">
		</jdbcConnection>
		
		<!--
		<jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
			connectionURL="jdbc:oracle:thin:@10.4.19.203:1521:xe"
			userId="tbsp" password="tbsp">
		</jdbcConnection>
-->
		<!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer true，把JDBC DECIMAL 和 
			NUMERIC 类型解析为java.math.BigDecimal -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- targetProject:自动生成代码的位置 -->
		<javaModelGenerator targetPackage="com.centaline.trans.task.entity"
			targetProject="src\main\java">
			<!-- enableSubPackages:是否让schema作为包的后缀 -->
			<property name="enableSubPackages" value="false" />
			<!-- 从数据库返回的值被清理前后的空格 -->
			<property name="trimStrings" value="true" /> 
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="task" targetProject="src\main\resources\mapper">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		
		<javaClientGenerator targetPackage="com.centaline.trans.task.repository" targetProject="src\main\java" type="XMLMAPPER" />
		
		<!-- <table domainObjectName="TsApiLog" tableName="T_TS_API_LOG" enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false" schema="sctrans" selectByExampleQueryId="false" /> -->
		<table domainObjectName="TlTaskSubmitLog" tableName="T_TL_TASK_SUBMIT_LOG" enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false" schema="sctrans" selectByExampleQueryId="false" />
	</context>

</generatorConfiguration>  