			<if test="changeDate != null">
				CHANGE_DATE,
			</if>
			<if test="changeUser != null">
				CHANGE_USER,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="pkid != null">
				#{pkid,jdbcType=BIGINT},
