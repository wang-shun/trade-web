	<select id="listByLoanId" resultMap="BaseResultMap"
		parameterType="java.lang.Long">
		select
		<include refid="Base_Column_List" />
		from sctrans.T_TO_LOAN_STATUS_CHANGE
		where LOAN_ID =
		#{loanId,jdbcType=BIGINT} 
